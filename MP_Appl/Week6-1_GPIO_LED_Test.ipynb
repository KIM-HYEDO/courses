    "import RPi.GPIO as GPIO\n",
    "import time\n",
    "\n",
    "GPIO.setmode(GPIO.BCM)\n",
    "GPIO.setup(5, GPIO.OUT)\n",
    "\n",
    "try:\n",
    "    while(True):\n",
    "        GPIO.output(5, True)\n",
    "        time.sleep(1)\n",
    "        GPIO.output(5, False)\n",
    "        time.sleep(1)\n",
    "finally:\n",
    "    print('cleaning up')\n",
    "    GPIO.cleanup()\n",
    "    "
